{
  "name": "@nteract/widget-environment",
  "version": "0.0.1",
  "description": "Provides an environment that widgets can run in and loads widget golden packages. Used within a cross domain or sanboxed iframe. Communicates by post message to window.top, where an outer shim must be running (transform-widgets).",
  "main": "dist/index.js",
  "nteractDesktop": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "webpack --config ./webpack.config.js",
    "build:watch": "npm run build:dist -- --watch"
  },
  "repository": "https://github.com/nteract/nteract/tree/master/packages/widget-environment",
  "keywords": [
    "nteract",
    "transforms",
    "notebook"
  ],
  "author": "Jonathan Frederic <jon.freder@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nteract/nteract/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@jupyter-widgets/jupyterlab-manager": "^0.38.1",
    "@jupyterlab/services": "^3.2.1",
    "@phosphor/coreutils": "^1.3.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "rxjs": "^6.3.3",
    "source-map": "^0.7.3",
    "ts-loader": "^5.2.1",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "peerDependencies": {
    "react": "^16.3.2"
  }
}
